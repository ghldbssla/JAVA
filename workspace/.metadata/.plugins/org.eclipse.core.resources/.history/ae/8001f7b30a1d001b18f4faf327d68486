package test;

import java.util.Scanner;

public class Test {
	public static void main(String[] args) {
		abs();
	}

	public static void abs() {
		Scanner sc = new Scanner(System.in);

		long a = 0;
		long b = 0;
		int c = 0;

		a=sc.nextInt();
		b=sc.nextInt();
		
		for (long i = 1; i <= b; i++) {
			for (long j = 1; j <= a; j++) {
				if(a*i==b*j) {
					System.out.println(a*i);
					c++;
					break;
				}
				if(c==1) {
					break;
				}
			}
		}
		
	}

}
