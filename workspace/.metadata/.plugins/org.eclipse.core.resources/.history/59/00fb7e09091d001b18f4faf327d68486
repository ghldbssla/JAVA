package test;

import java.util.Scanner;

public class Test {
	public static void main(String[] args) {
		abs();
	}

	public static void abs() {
		Scanner sc = new Scanner(System.in);

		long a = 0;
		long b = 0;
		long c = 0;

		a=sc.nextInt();
		b=sc.nextInt();
		c=sc.nextInt();
		
		if(b<=a&&b>=c||b>=a&&b<=c) {
			System.out.println(b);
		}else if(a<=b&&a>=c||c>=a&&c<=b) {
			System.out.println(c);
		}else {
			System.out.println(a);
		}
		
	}

}
