package test;

import java.util.Scanner;

public class Test {
	public static void main(String[] args) {
		abs();
	}

	public static void abs() {
		Scanner sc = new Scanner(System.in);

		String a = "";
		String b = "";
		a = sc.next();
		for (int i = a.length(); i > 0; i--) {
			b+=a.charAt(i);
		}
		System.out.println(b);
	}

}
