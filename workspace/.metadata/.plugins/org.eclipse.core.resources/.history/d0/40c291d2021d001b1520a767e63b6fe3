package test;

import java.util.Scanner;

public class Test {
	public static void main(String[] args) {
		abs();
	}

	public static void abs() {
		Scanner sc = new Scanner(System.in);
		
		int a=0;
		int c=0;
		a=sc.nextInt();
		int f=0;
		int[] b=new int[a];
		for (int i = 0; i < a; i++) {
			b[i]=sc.nextInt();
		}
		c=sc.nextInt();
		for (int i = 0; i < a; i++) {
			if(b[i]==c) {
				System.out.println(i+1);
				f+=1;
				break;
			}
		}
		if(f==0) {
			System.out.println(0);
		}
	}

}
