package test;

import java.util.Scanner;

public class Test {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = 0;
		int b = 0;
		a = sc.nextInt();
		for (int i = a; i >= 1; i--) {
			for (int j = 1; j <= a; j++) {
				if(j <= i) {
					System.out.print(" ");
				}else {
					System.out.print("*\n");
				}
			}
			System.out.println("");

		}

	}
}
