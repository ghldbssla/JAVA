package test;

import java.util.Scanner;

public class Test {
	public static void main(String[] args) {
		abs();
	}

	public static void abs() {
		Scanner sc = new Scanner(System.in);

		long a = 0;
		long b = 0;
		a = sc.nextInt();
		b = sc.nextInt();
		if(a-b>0) {
		System.out.println(a-b);
		}else {
			System.out.println(-a+b);
			
		}
	}

}
